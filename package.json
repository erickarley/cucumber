{
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "name": "cucumber-boilerplate",
  "version": "2.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "contributors": [
    "W. van Kuipers <github@pwnd.nl>"
  ],
  "repository": {
    "type": "git",
    "getUrl": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "bugs": {
    "getUrl": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "wdio",
    "webdriver",
    "webdriverio",
    "cucumber",
    "test",
    "boilerplate",
    "selenium"
  ],
  "scripts": {
    "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
    "test": "run-s test:*",
    "test:lint": "eslint 'src/**/*.ts' 'test/**/*.js!(setup.js)'",
    "test:unit": "jest --config=jest.config.js --detectOpenHandles",
    "test:features": "wdio run wdio.BUILD.conf.ts",
    "wdio": "wdio run wdio.conf.ts",
    "postinstall": "rimraf -rm node_modules/wdio-html-nice-reporter/node_modules/@wdio/types"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@wdio/cli": "^7.16.15",
    "@wdio/config": "^7.16.14",
    "@wdio/cucumber-framework": "^7.16.14",
    "@wdio/junit-reporter": "^7.25.1",
    "@wdio/local-runner": "^7.16.15",
    "@wdio/selenium-standalone-service": "^7.16.14",
    "@wdio/shared-store-service": "^7.25.4",
    "@wdio/spec-reporter": "^7.16.14",
    "@wdio/static-server-service": "^7.16.14",
    "@wdio/types": "^7.8.0",
    "chromedriver": "^98.0.1",
    "cucumber-junit-convert": "^2.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-wdio": "^7.4.2",
    "http-server": "^14.1.0",
    "imap-simple": "^5.1.0",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "mailparser": "^3.7.5",
    "npm-run-all": "^4.1.5",
    "pdf2json": "^4.0.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "resemblejs": "^5.0.0",
    "tesseract.js": "^6.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "wdio-chromedriver-service": "^7.2.8",
    "wdio-cucumberjs-json-reporter": "^4.4.3",
    "wdio-html-nice-reporter": "^8.0.0",
    "wdio-image-comparison-service": "^3.1.1",
    "webdriverio": "^7.16.15",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@rpii/wdio-html-reporter": "^7.7.1",
    "@wdio/reporter": "^8.6.8",
    "child_process": "^1.0.2",
    "msnodesqlv8": "^4.2.1",
    "mssql": "^11.0.1",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "nodemailer": "^6.7.5",
    "wdio-timeline-reporter": "^5.1.4"
  }
}
